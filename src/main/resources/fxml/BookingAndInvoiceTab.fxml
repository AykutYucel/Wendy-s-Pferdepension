<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="567.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.tuwien.sepm.assignment.individual.fleet.ui.BookingAndInvoiceTabController">
   <children>
      <TableView fx:id="Booking_TV" prefHeight="439.0" prefWidth="597.0">
        <columns>
          <TableColumn fx:id="Name_TC" prefWidth="111.0" text="Customer Name" />
          <TableColumn fx:id="Beginning_TC" prefWidth="161.0" text="Beginning" />
            <TableColumn fx:id="Ending_TC" prefWidth="161.0" text="Ending" />
            <TableColumn fx:id="Status_TC" prefWidth="68.0" text="Status" />
            <TableColumn fx:id="Price_TC" prefWidth="95.0" text="Total Price" />
        </columns>
      </TableView>
      <ChoiceBox fx:id="Type_CB" layoutX="648.0" layoutY="38.0" prefHeight="27.0" prefWidth="131.0" />
      <Label layoutX="600.0" layoutY="43.0" text="Type" />
      <Button fx:id="Complete_Button" layoutX="615.0" layoutY="475.0" mnemonicParsing="false" onAction="#onCompleteButtonClicked" prefHeight="27.0" prefWidth="113.0" text="Complete" />
      <Button fx:id="Detail_Button" layoutX="410.0" layoutY="475.0" mnemonicParsing="false" onAction="#onDetailsButtonClicked" prefHeight="27.0" prefWidth="113.0" text="View Details" />
      <Button fx:id="Cancel_Button" layoutX="224.0" layoutY="475.0" mnemonicParsing="false" onAction="#onCancelButtonClicked" text="Cancel Booking" />
      <Button fx:id="Edit_Button" layoutX="49.0" layoutY="475.0" mnemonicParsing="false" onAction="#onEditButtonClicked" prefHeight="27.0" prefWidth="113.0" text="Edit Booking" />
   </children>
</AnchorPane>
